{
  "prefix": "http header",
  "body": [
    "curl --request ${1|GET,POST,DELETE,PUT|} -sL \\",
    "\t--user-agent '${2|Shellman,Your custom user agent|}' \\",
    "\t--header ${3|'key: value',\"${key}\": \"${value}\"|} \\",
    "\t--url ${4|'http://example.com',\"${url}\"|}\n"
