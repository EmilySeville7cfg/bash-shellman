{
  "prefix": [
    "archive decompress tar.gz",
    "decompress tar.gz"
  ],
  "body": "tar ${1|-C,--directory|} ${2|/extract/to/path, \"${extractToPath}\"|} ${3|-xzvf,--extract --gzip --verbose --file|} ${4|/path/to/archive, \"${pathToArchive}\"|}.tar.gz\n",
  "description": "decompress a .tar.gz file to the specified path"
}