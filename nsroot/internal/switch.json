{
  "prefix": "switch case",
  "body": [
    "case \"\\${${1:item}\\}\" in",
    "\t${2:1})",
    "\t\t${3:echo \"item = 1\"}",
    "\t;;",
    "\t${4:2|3})",
    "\t\t${5:echo \"item = 2 or item = 3\"}",
    "\t;;",
    "\t*)",
    "\t\t${6:echo \"default (none of above)\"}",
    "\t;;",
    "esac\n"
  ],
  "description": "switch case"
}