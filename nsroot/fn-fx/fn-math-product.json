{
  "prefix": "fn math product",
  "body": [
    "# Usage: product int1 int2 ...",
    "function product () {",
    "\tlocal result=1",
    "\tfor int in $@; do",
    "\t\t((result *= int))",
    "\tdone",
    "\techo \\${result\\}",
    "}\n"
  ],
  "description": "calculate product of given integers"
}